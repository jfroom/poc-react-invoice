{
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jest": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
       "semi": ["warn", "never"],
       "react/jsx-filename-extension": [0, "never"],
       "react/no-array-index-key": [0, "never"]
    }
}
